<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>

            <resource id="Magento_Backend::admin">
                <resource id="Magento_Catalog::catalog" title="catalog">
                    <resource id="Magento_Catalog::catalog_inventory" title="catalog Inventory">
                        <resource id="Boostmyshop_Barcodeinventory::Main" title="Barcode Inventory" sortOrder="40" />
                    </resource>
                </resource>

                <resource id="Magento_Backend::stores" title="store">
                    <resource id="Magento_Backend::stores_settings" title="store setting">
                        <resource id="Magento_Config::config" title="config">
                            <resource id="Boostmyshop_BarcodeInventory::Config" title="Barcode Inventory section" />
                        </resource>
                    </resource>
                </resource>
            </resource>


        </resources>
    </acl>
</config>
